{
	"name": "moaction/jsonrpc-bundle",
	"description": "Jsonrpc server and client Symfony2 bundle",
	"minimum-stability": "stable",
	"license": "MIT",
	"authors": [
		{
			"name": "Alexey Kalinin",
			"email": "alexey.kalinin@moaction.ru"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"moaction/jsonrpc-server": "dev-master",
		"moaction/jsonrpc-client": "dev-master"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@git.moteam.ru:jsonrpc/common.git"
		},
		{
			"type": "vcs",
			"url": "git@git.moteam.ru:jsonrpc/server.git"
		},
		{
			"type": "vcs",
			"url": "git@git.moteam.ru:jsonrpc/client.git"
		}
	],
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
	"autoload": {
		"psr-0": {"Moaction\\": "src/"}
	}
}
